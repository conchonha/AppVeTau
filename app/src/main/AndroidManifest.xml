<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cj.trainticks">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.TrainTicks">
        <activity android:name=".src.page.traindetail.TrainDetailActivity"/>
        <activity android:name=".src.page.user.RegisterActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".src.page.home.TuyenTauActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".src.page.home.ShowListChuyenTauActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".src.page.home.ChonVeTauActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".src.page.home.ThongTinDatVeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".src.page.splash.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".src.page.user.LoginActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".src.page.main.MainActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>